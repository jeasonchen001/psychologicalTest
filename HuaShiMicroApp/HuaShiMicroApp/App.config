<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--显示的必读须知-->
    <add key="information" value = 
        "   首先屏幕中央会呈现一个注视点，接着屏幕中央会呈现一张图片，然后在屏幕其他位置会出现第二张图片，请你尽量又快又准地识别第二张图片中的内容，如果是人脸请按“←”键，如果是物体（除人脸外）请按“→”键。明白了请按Q键开始实验。" />
    <!--第一组图片显示的时间, 单位ms-->
    <add key="image1DisplayTime" value = "1000" />
    <!--聚焦点显示的时间，单位ms-->
    <add key="image1DisableTime" value = "300" />
    <!--第一组的名称-->
    <add key="group1Name" value="人物"/>
    <!--第二组名称-->
    <add key="group2Name" value="动物"/>
  </appSettings>
</configuration>